{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap":true,
        "noImplicitAny": false,
        "module":"commonjs",
        "target": "es5",
        "jsx": "react",

        "allowJs": true,
        "moduleResolution": "node",
        "experimentalDecorators": true,
        "emitDecoratorMetadata": true,
        "allowSyntheticDefaultImports": true,
        "reactNamespace": "React"
    },

    "exclude": [
        "node_modules",
        "typings"
    ]
}